{
  "connection": "default",
  "collectionName": "trips",
  "info": {
    "name": "trip",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "ticketSales": {
      "unique": true,
      "required": true,
      "type": "date"
    },
    "tripDate": {
      "required": true,
      "type": "date"
    },
    "tripName": {
      "type": "string",
      "required": true
    },
    "tripDescription": {
      "required": true,
      "type": "text"
    },
    "tripPhoto": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "featuredTrip": {
      "model": "about",
      "via": "featuredTrips"
    },
    "users": {
      "collection": "user",
      "via": "trips",
      "dominant": true,
      "plugin": "users-permissions"
    },
    "ticketLocation": {
      "default": "DAC Lobby",
      "type": "string"
    },
    "isOnlineSale": {
      "required": true,
      "type": "boolean"
    },
    "ticketLink": {
      "type": "string"
    }
  }
}