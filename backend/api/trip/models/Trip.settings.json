{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "trips",
  "info": {
    "name": "trip",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "ticketSales": {
      "unique": true,
      "required": true,
      "type": "datetime"
    },
    "tripDate": {
      "required": true,
      "type": "datetime"
    },
    "tripName": {
      "type": "string",
      "required": true
    },
    "tripDescription": {
      "required": true,
      "type": "text"
    },
    "tripPhoto": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "trips",
      "dominant": true
    },
    "ticketLocation": {
      "default": "Rec Lobby",
      "type": "string"
    },
    "isOnlineSale": {
      "required": true,
      "type": "boolean"
    },
    "ticketLink": {
      "type": "string"
    },
    "importantDocuments": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
