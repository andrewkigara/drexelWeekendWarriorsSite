{
  "connection": "default",
  "collectionName": "abouts",
  "info": {
    "name": "about",
    "description": "About us page information"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "clubName": {
      "type": "string"
    },
    "clubDescription": {
      "required": true,
      "type": "text"
    },
    "upcomingTrips": {
      "collection": "trip",
      "via": "featuredTrip"
    },
    "announcementMessage": {
      "type": "string"
    },
    "announcementLink": {
      "type": "string"
    }
  }
}