{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "forms",
  "info": {
    "name": "form",
    "description": "The ticket forms for each user used for trips"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "trip": {
      "model": "trip",
      "via": "participantForms"
    },
    "participantInfo": {
      "type": "component",
      "repeatable": false,
      "component": "forms.trip-participant-info",
      "required": true
    },
    "participantMedicalInfo": {
      "type": "component",
      "repeatable": false,
      "component": "forms.trip-participant-medical-info",
      "required": true
    },
    "formToken": {
      "type": "string",
      "private": true,
      "required": true,
      "default": "hashCode"
    }
  }
}
