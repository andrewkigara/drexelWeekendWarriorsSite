{
  "connection": "default",
  "collectionName": "messages",
  "info": {
    "name": "message",
    "description": "Messages that can be sent between users"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "Message": {
      "required": true,
      "type": "text"
    },
    "from": {
      "model": "user",
      "plugin": "users-permissions"
    },
    "to": {
      "model": "user",
      "plugin": "users-permissions"
    }
  }
}