{
  "connection": "default",
  "collectionName": "messages",
  "info": {
    "name": "message",
    "description": "An email to send to drexelweekendwarriors@gmail.com"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "firstName": {
      "required": true,
      "type": "string"
    },
    "lastName": {
      "required": true,
      "type": "string"
    },
    "emailFrom": {
      "required": true,
      "type": "email"
    },
    "message": {
      "required": true,
      "type": "text"
    },
    "subject": {
      "required": true,
      "type": "string"
    }
  }
}